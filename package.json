    "clsx": "^2.1.1",
    "easymde": "^2.18.0",
    "lucide-react": "^0.474.0",
    "next": "15.1.6",
    "next-auth": "^5.0.0-beta.25",
    "next-sanity": "^9.8.47",
    "nodemailer": "^6.10.0",
